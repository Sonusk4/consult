{
  "name": "consultancy-platform",
  "version": "1.0.0",
  "description": "Full-stack consultancy platform connecting users with verified consultants",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend-new && npm run dev",
    "install:all": "npm install && cd frontend && npm install && cd ../backend-new && npm install",
    "build:frontend": "cd frontend && npm run build",
    "start": "cd backend-new && npm start",
    "start:backend": "cd backend-new && npm start"
  },
  "keywords": ["consultancy", "platform", "react", "nodejs", "video-calls", "booking"],
  "author": "ConsultaPro Team",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {},
  "workspaces": ["frontend", "backend-new"]
}
